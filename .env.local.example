# Copy this file to .env.local and update with your Supabase credentials

# Your Supabase project URL
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here

# Your Supabase anon/public key  
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Custom brand name (defaults to WilliamsHoldings)
NEXT_PUBLIC_BRAND_NAME=WilliamsHoldings

# Optional: Custom primary color (defaults to #059669)
NEXT_PUBLIC_PRIMARY_HEX=#059669

# Optional: Default currency (defaults to USD)
NEXT_PUBLIC_DEFAULT_CURRENCY=USD
